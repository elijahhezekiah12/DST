class Stack():

	def __init__(self):
		self.items = []

	def isEmpty(self):
		if not self.items:
			return True
		else:
			return False
		
	def push(self, item):
		self.items.append(item)
	def pop(self):
		return self.items.pop()
	def peek(self):
		if not self.items:
			return "Empty"
		else:
			return self.items[len(self.items) - 1]
	def size():
		return len(self.items)

if __name__ == "__main__":
	stack = Stack()
	stack.push(1)
	print stack.isEmpty()
	print stack.peek()
	stack.push(2)
	print stack.size()
